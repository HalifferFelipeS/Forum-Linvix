generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Article {
  id        String   @id @default(cuid())
  title     String
  categories String[]
  content   String
  videoId   String?
  pdfUrl    String?
  createdAt DateTime @default(now())
}